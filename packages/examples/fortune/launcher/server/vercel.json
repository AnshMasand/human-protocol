{
  "buildCommand": "yarn workspace @human-protocol/job-launcher-server build",
  "outputDirectory": "build",
  "installCommand": "yarn install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "index.js"
    }
  ],
  "functions": {
    "index.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}
