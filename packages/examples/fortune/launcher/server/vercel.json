{
  "buildCommand": "yarn workspace @human-protocol/job-launcher-server build",
  "installCommand": "yarn install",
  "outputDirectory": "build",
  "functions": {
    "src/index.ts": {
      "runtime": "@vercel/node",
      "maxDuration": "60"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "src/index.ts"
    }
  ]
}
