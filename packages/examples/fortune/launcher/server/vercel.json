{
    "buildCommand": "yarn workspace @human-protocol/job-launcher-server build",
    "outputDirectory": "build",
    "installCommand": "yarn workspace @human-protocol/job-launcher-server install",
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.ts"
        }
    ]
  }